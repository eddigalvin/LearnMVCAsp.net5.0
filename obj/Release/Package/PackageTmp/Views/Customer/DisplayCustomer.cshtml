@model MVCHelloWorld2.ViewModel.CustomerViewModel
@using MVCHelloWorld2.Models;
<table>
    <tr>
        <td>Customer Code</td>
        <td>Customer Name</td>
    </tr>
    @foreach (Customer o in Model.customers)
    {
        <tr>
            <td>@o.CustomerCode</td>
            <td>@o.CustomerName</td>
        </tr>
    }
</table> 

